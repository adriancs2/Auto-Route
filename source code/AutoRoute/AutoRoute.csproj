<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net48</TargetFrameworks>
    <RootNamespace>System</RootNamespace>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageId>adriancs.AutoRoute</PackageId>
    <Title>Auto Route</Title>
    <Authors>adriancs</Authors>
    <Description>Automatic route all ASP.NET pages (.aspx).</Description>
    <Copyright>adriancs</Copyright>
    <PackageProjectUrl>https://github.com/adriancs2/Auto-Route</PackageProjectUrl>
    <PackageTags>routing;route</PackageTags>
    <PackageReleaseNotes>Automatic route all ASP.NET pages (.aspx).</PackageReleaseNotes>
    <PackageLicenseExpression>Unlicense</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="System.Web" />
    <Reference Include="System.Web.Routing" />
  </ItemGroup>

</Project>
